package pack.cap.module6.inter;
/*
 * Declare it , instantiate the object , Access and use the object 
 * */

class Student{
	String name ; int age ;

	public Student(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	} 
	public void displayInfo() {
		System.out.println("Name : "+ name +" Age : "+ age );
	}
}




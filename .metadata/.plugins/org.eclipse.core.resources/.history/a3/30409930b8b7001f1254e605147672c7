	private int outerValue = 100;
	  class Inner {
		   void disp() {
			   System.out.println("Outer class value : "+ outerValue);
		   }
	  }
}


public class OuterClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

package pack.cap.module3.ctrl_stmt;

public class WhileLoop_Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int row = 1 ;
		while(row <= 10) {
			int column = 1;
			while (column <=10 ) {
				int result = row*column ;
				System.out.println(result +" \t");
				column++;
			}
			System.out.println();
			row++;
			
		}
		
	}

}

package pack.cap.module3.ctrl_stmt;

public class SearchTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int nums[] = {1,8,3,7,5,6,10,4};
		int val = 10;
		boolean found = false;
		
		for(int x : nums) {
			if(x == val ) {
				found = true ;
				break ;
			}
		}
		if(found)
			System.out.println("Value found !");

	
	for(int i =1; i<=10 ;i++) {
		if(i%2 !=0) {
			continue;
		}
		System.out.println(i);
		
	}

}
}

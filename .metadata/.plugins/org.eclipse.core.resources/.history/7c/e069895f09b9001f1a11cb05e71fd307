package pack.cap.module6.inter;

class A {
	void m1() {System.out.println("A");}
}
class B extends A{
	void m1() {System.out.println("b");}
}

class C extends B{
	void m1() {System.out.println("c");}
}
public class Demo {
	 public void infoStudent(int age, String loc, int phoneNo ) 
	  { 
	    
	  } 
	 public static void main(String[] args) {
	 A a = new A();
	 a.m1(); //a
	 
	 B b = new  B();
	 b.m1(); //b
	 
	 C c = new C();
	 c.m1();//c
	 
	 A a2;
	 a2=b; 
	 a2.m1(); 
	 
	 
	 
	 
	 
	 }
}
